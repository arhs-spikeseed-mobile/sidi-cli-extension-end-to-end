  "scripts": {
    "e2e:proj:download_last_apps_with": "sh lib/scripts/browserstack/e2e.browserstack.download_app_with.sh",
    "dev:prettier": "prettier --config ./.prettierrc --check \"../__e2e__/**/*.ts\"",
    "dev:prettier:check": "prettier --config ./.prettierrc --check \"../__e2e__/**/*.ts\"",
    "dev:prettier:fix": "prettier --config ./.prettierrc --write --check \"../__e2e__/**/*.ts\"",
    "e2e:browserstack:software:browserstacklocal": "sh lib/scripts/browserstack/e2e.browserstack.start_browserstacklocal.sh",
    "e2e:browserstack:run:local": "sh lib/scripts/browserstack/e2e.browserstack.local_run_script.sh browserstack",
    "e2e:simulator:run:local": "sh lib/scripts/browserstack/e2e.browserstack.local_run_script.sh simulator",
    "e2e:simulator:ios_list": "xcrun simctl list devices | grep -v '(unavailable' ",
    "e2e:simulator:android_list": "emulator -list-avds",
    "e2e:simulator:android_start_emulator": "emulator -avd end-to-end-device",
    "e2e:browserstack:run:local:prompt": "sh lib/scripts/browserstack/e2e.browserstack.prompt_local_run_script.sh"
  },
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.25.9",
    "@babel/preset-env": "7.25.9",
    "@babel/register": "7.25.9",
    "@babel/runtime": "7.25.9",
    "@rpii/wdio-html-reporter-pdf": "^8.0.7",
    "@types/node": "22.10.2",
    "@wdio/appium-service": "9.4.2",
    "@wdio/browserstack-service": "9.4.2",
    "@wdio/cli": "9.4.2",
    "@wdio/globals": "9.4.2",
    "@wdio/junit-reporter": "9.4.2",
    "@wdio/local-runner": "9.4.2",
    "@wdio/mocha-framework": "9.4.2",
    "@wdio/spec-reporter": "9.4.2",
    "appium": "^2.13.1",
    "appium-uiautomator2-driver": "^3.9.5",
    "appium-xcuitest-driver": "^7.34.1",
    "browserstack-local": "^1.5.5",
    "prettier": "^3.4.2",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "wdio-html-nice-reporter": "^8.1.7",
    "wdio-performancetotal-service": "3.1.0",
    "undici": "5.28.4",
    "sidi-cli": "3.1.0"
